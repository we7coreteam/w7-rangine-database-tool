{
	"name": "w7/rangine-database-tool",
	"description": "rangine database-tool.",
	"keywords": [
		"rangine",
		"database-tool"
	],
	"license": "Apache-2.0",
	"require": {
		"php": ">=7.1.0",
		"w7/rangine": "~3.0.0",
		"doctrine/dbal": "~2.0"
	},
	"require-dev": {
		"w7/php-cs-fixer": "*"
	},
	"autoload": {
		"psr-4": {
			"W7\\DatabaseTool\\": "src/"
		}
	},
	"extra": {
		"rangine": {
			"providers": [
				"W7\\DatabaseTool\\ServiceProvider"
			]
		}
	},
	"config": {
		"allow-plugins": {
			"w7/php-cs-fixer": true,
			"funkjedi/composer-include-files": true,
			"w7/rangine-package-plugin": true
		}
	}
}
